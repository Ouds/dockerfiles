{
  "expo": {
    "name": "typescript-expo-apollo-firebase-tabs",
    "description": "typescript-expo-apollo-firebase-tabs",
    "slug": "typescript-expo-apollo-firebase-tabs",
    "version": "0.0.1",
    "sdkVersion": "32.0.0",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "splash": {
      "backgroundColor": "#fff",
      "image": "./assets/splash-screen.png",
      "resizeMode": "contain"
    },
    "primaryColor": "#fff",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "packagerOpts": {
      "assetExts": [
        "ttf",
        "mp4"
      ]
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "githubUrl": "https://github.com/zzy/typescript-expo-apollo-firebase-tabs"
  }
}


